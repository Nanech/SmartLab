<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mediator_nav"
    app:startDestination="@id/navigation_analyzes">

    <fragment
        android:id="@+id/navigation_analyzes"
        android:name="com.example.smartlab.fragments.AnalyzesFragment"
        android:label="@string/title_analyzes"
        tools:layout="@layout/fragment_analyzes" />
    <fragment
        android:id="@+id/navigation_results"
        android:name="com.example.smartlab.fragments.ResultsFragment"
        android:label="@string/title_results"
        tools:layout="@layout/fragment_results" />
    <fragment
        android:id="@+id/navigation_support"
        android:name="com.example.smartlab.fragments.SupportFragment"
        android:label="@string/title_support"
        tools:layout="@layout/fragment_support" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.smartlab.fragments.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

</navigation>